                {
                    "@type": "m4i:NumericalVariable",
                    "m4i:hasNumericalValue": "16",
                    "pivmeta:hasStandardName": "https://matthiasprobst.github.io/pivmeta#x_final_interrogation_window_size",
                    "@id": "local:b9e86d60-8f01-40ab-b1e8-3dc781872605"
                },
                {
                    "@type": "m4i:NumericalVariable",
                    "m4i:hasNumericalValue": "16",
