local:arithmetic_mean_of_image_particle_diameter a ssno:StandardName ;
    ssno:description "Arithmetic mean of the image particle diameters in the flow. The diameter of an image particle approximated by a circular Gaussian intensity bell, where the diameter is defined at the e-2 intensity, thus the particle diameter is equal to 4 sigma of the Gaussian distribution. Note, that there might be other definitions, too, however, this one is the most common one in literature." ;
    ssno:standardName "arithmetic_mean_of_image_particle_diameter" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:arithmetic_mean_of_seeding_particle_size a ssno:StandardName ;
    ssno:description "The arithmetic mean of the diameter of the seeding particles." ;
    ssno:standardName "arithmetic_mean_of_seeding_particle_size" ;
    ssno:unit <http://qudt.org/vocab/unit/M> .

local:ccd_height a ssno:StandardName ;
    ssno:description "The height of a ccd pixel." ;
    ssno:standardName "ccd_height" ;
    ssno:unit <http://qudt.org/vocab/unit/M> .

local:ccd_width a ssno:StandardName ;
    ssno:description "The width of a ccd pixel." ;
    ssno:standardName "ccd_width" ;
    ssno:unit <http://qudt.org/vocab/unit/M> .

local:displacement a ssno:StandardName ;
    ssno:description "PIV pixel displacement magnitude in the image plane." ;
    ssno:standardName "displacement" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:final_interrogation_window_overlap_size a ssno:StandardName ;
    ssno:description "Width and height of a square final PIV interrogation window overlap." ;
    ssno:standardName "final_interrogation_window_overlap_size" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:final_interrogation_window_size a ssno:StandardName ;
    ssno:description "Width and height of a square final PIV interrogation window." ;
    ssno:standardName "final_interrogation_window_size" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:first_peak_x_displacement a ssno:StandardName ;
    ssno:description "Displacement is a vector quantity. X indicates the component in x-axis direction. This component is an in-plane component. The displacement is computed based on the first peak of the cross-correlation function." ;
    ssno:standardName "first_peak_x_displacement" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:first_peak_y_displacement a ssno:StandardName ;
    ssno:description "Displacement is a vector quantity. Y indicates the component in y-axis direction. This component is an in-plane component. The displacement is computed based on the first peak of the cross-correlation function." ;
    ssno:standardName "first_peak_y_displacement" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:focal_length a ssno:StandardName ;
    ssno:description "The focal length of a single lens or a lens system like an objective. See also https://www.wikidata.org/wiki/Q193540." ;
    ssno:standardName "focal_length" ;
    ssno:unit <http://qudt.org/vocab/unit/M> .

local:image_bit_depth a ssno:StandardName ;
    ssno:description "The pixel depth of an image in bits. Typical bit depth are 8 bits or 16 bits." ;
    ssno:standardName "image_bit_depth" ;
    ssno:unit <http://qudt.org/vocab/unit/UNITLESS> .

local:image_density a ssno:StandardName ;
    ssno:description "The image density is computed based on the source density $N_s$ and the interrogation window size." ;
    ssno:standardName "image_density" ;
    ssno:unit <http://qudt.org/vocab/unit/UNITLESS> .

local:image_filter_kernel_size a ssno:StandardName ;
    ssno:description "The size of the image filter kernel." ;
    ssno:standardName "image_filter_kernel_size" ;
    ssno:unit <http://qudt.org/vocab/unit/UNITLESS> .

local:image_index a ssno:StandardName ;
    ssno:description "Index of a PIV image collection. Starts at 1 and ends at the number of images N." ;
    ssno:standardName "image_index" ;
    ssno:unit <http://qudt.org/vocab/unit/UNITLESS> .

local:image_particle_density a ssno:StandardName ;
    ssno:description "Number of particle images per pixel. Equals the number of particles divided by the image area." ;
    ssno:standardName "image_particle_density" ;
    ssno:unit <https://matthiasprobst.github.io/pivmeta#PER-PIXEL> .

local:image_particle_diameter a ssno:StandardName ;
    ssno:description "The diameter of an image particle approximated by a circular Gaussian intensity bell, where the diameter is defined at the e-2 intensity, thus the particle diameter is equal to 4 sigma of the Gaussian distribution. Note, that there might be other definitions, too, however, this one is the most common one in literature." ;
    ssno:standardName "image_particle_diameter" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:initial_interrogation_window_size a ssno:StandardName ;
    ssno:description "Width and height of a square initial PIV interrogation window (in case of a multi-grid PIV algorithm)." ;
    ssno:standardName "initial_interrogation_window_size" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:inplane_velocity a ssno:StandardName ;
    ssno:description "The standard name inplane_velocity denotes the magnitude of a two-dimensional velocity vector, which . Plane is spanned by the x and y axis of PIV plane. The PIV plane is the plane illuminated by the laser. The component in the third direction is called outofplane_velocity." ;
    ssno:standardName "inplane_velocity" ;
    ssno:unit <http://qudt.org/vocab/unit/M-PER-SEC> .

local:laser_pulse_delay a ssno:StandardName ;
    ssno:description "Time between two laser pulses." ;
    ssno:standardName "laser_pulse_delay" ;
    ssno:unit <http://qudt.org/vocab/unit/SEC> .

local:laser_sheet_thickness a ssno:StandardName ;
    ssno:description "The laser sheet thickness used in a measurement. For a model laser sheet thickness, use model_laser_sheet_thickness. The use of laser_sheet_thickness has no clear definition, but is where the intensity of the laser sheet drops significantly." ;
    ssno:standardName "laser_sheet_thickness" ;
    ssno:unit <http://qudt.org/vocab/unit/M> .

local:least_square_residual_of_z_displacement_reconstruction a ssno:StandardName ;
    ssno:description "The least square residual of the z displacement reconstruction in stereo PIV." ;
    ssno:standardName "least_square_residual_of_z_displacement_reconstruction" ;
    ssno:unit <http://qudt.org/vocab/unit/UNITLESS> .

local:magnitude_of_velocity a ssno:StandardName ;
    ssno:description "Velocity is defined as a three-dimensional velocity vector. Magnitude denotes the length of the vector." ;
    ssno:standardName "magnitude_of_velocity" ;
    ssno:unit <http://qudt.org/vocab/unit/M-PER-SEC> .

local:model_laser_sheet_shape_factor a ssno:StandardName ;
    ssno:description "The shape factor used to model the expanded laser beam. The shape factor is introduced in 'Particle Image Velocimetry A Practical Guide' by Raffel et al. (2018). It deforms a Gaussian beam. High values result in a more top-hat-like beam, while low values result in a more Gaussian-like beam." ;
    ssno:standardName "model_laser_sheet_shape_factor" ;
    ssno:unit <http://qudt.org/vocab/unit/UNITLESS> .

local:model_laser_sheet_thickness a ssno:StandardName ;
    ssno:description "The width of the laser sheet illuminating the seeding particles as used in a model (synthetic PIV. The width is defined at the e-2 intensity of a Gaussian bell curve. However, the laser beam shape must not be Gaussian, but can be more top-hat-like. The shape is defined by the shap factor (laser_sheet_shape_factor)." ;
    ssno:standardName "model_laser_sheet_thickness" ;
    ssno:unit <http://qudt.org/vocab/unit/M> .

local:number_of_particles a ssno:StandardName ;
    ssno:description "Number of seeding particles in the image or field of interest. See also image_particle_density." ;
    ssno:standardName "number_of_particles" ;
    ssno:unit <http://qudt.org/vocab/unit/UNITLESS> .

local:optical_magnification a ssno:StandardName ;
    ssno:description "The optical magnification of the imaging system." ;
    ssno:standardName "optical_magnification" ;
    ssno:unit <https://matthiasprobst.github.io/pivmeta#MilliM-PER-PIXEL> .

local:piv_correlation_coefficient a ssno:StandardName ;
    ssno:description "The correlation coefficient is a measure for the quality of the PIV evaluation. It is computed based on the cross-correlation of the interrogation windows." ;
    ssno:standardName "piv_correlation_coefficient" ;
    ssno:unit <http://qudt.org/vocab/unit/UNITLESS> .

local:piv_first_peak_correlation_coefficient a ssno:StandardName ;
    ssno:description "The correlation coefficient is a measure for the quality of the PIV evaluation. It is computed based on the cross-correlation of the interrogation windows. The first peak correlation coefficient is the correlation coefficient of the first peak of the cross-correlation function." ;
    ssno:standardName "piv_first_peak_correlation_coefficient" ;
    ssno:unit <http://qudt.org/vocab/unit/UNITLESS> .

local:piv_scaling_factor a ssno:StandardName ;
    ssno:description "Image scaling factor." ;
    ssno:standardName "piv_scaling_factor" ;
    ssno:unit <http://qudt.org/vocab/unit/PER-M> .

local:piv_second_peak_correlation_coefficient a ssno:StandardName ;
    ssno:description "The correlation coefficient is a measure for the quality of the PIV evaluation. It is computed based on the cross-correlation of the interrogation windows. The first peak correlation coefficient is the correlation coefficient of the second peak of the cross-correlation function." ;
    ssno:standardName "piv_second_peak_correlation_coefficient" ;
    ssno:unit <http://qudt.org/vocab/unit/UNITLESS> .

local:piv_third_peak_correlation_coefficient a ssno:StandardName ;
    ssno:description "The correlation coefficient is a measure for the quality of the PIV evaluation. It is computed based on the cross-correlation of the interrogation windows. The first peak correlation coefficient is the correlation coefficient of the third peak of the cross-correlation function." ;
    ssno:standardName "piv_third_peak_correlation_coefficient" ;
    ssno:unit <http://qudt.org/vocab/unit/UNITLESS> .

