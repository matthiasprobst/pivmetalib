    ssno:standardName "saturated_pixel_number" ;
    ssno:unit <http://qudt.org/vocab/unit/UNITLESS> .

local:second_peak_x_displacement a ssno:StandardName ;
    ssno:description "Displacement is a vector quantity. X indicates the component in x-axis direction. This component is an in-plane component. The displacement is computed based on the second peak of the cross-correlation function." ;
    ssno:standardName "second_peak_x_displacement" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:second_peak_y_displacement a ssno:StandardName ;
    ssno:description "Displacement is a vector quantity. Y indicates the component in y-axis direction. This component is an in-plane component. The displacement is computed based on the second peak of the cross-correlation function." ;
    ssno:standardName "second_peak_y_displacement" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:sensor_pixel_height a ssno:StandardName ;
    ssno:description "The height of the sensor in pixel." ;
    ssno:standardName "sensor_pixel_height" ;
    ssno:unit <http://qudt.org/vocab/unit/M> .

local:sensor_pixel_height_fill_factor a ssno:StandardName ;
    ssno:description "Fill factor in vertical direction. The fill factor of an image sensor array is the ratio of a pixel's light sensitive area to its total area. See https://en.wikipedia.org/wiki/Fill_factor_(image_sensor). The value must be between 0 and 1, where 1 depicts a 100 percent. The higher the fill factor, the more sensitive the sensor." ;
    ssno:standardName "sensor_pixel_height_fill_factor" ;
    ssno:unit <http://qudt.org/vocab/unit/UNITLESS> .

local:sensor_pixel_width a ssno:StandardName ;
    ssno:description "The width of the sensor in pixel." ;
    ssno:standardName "sensor_pixel_width" ;
    ssno:unit <http://qudt.org/vocab/unit/M> .

local:sensor_pixel_width_fill_factor a ssno:StandardName ;
    ssno:description "Fill factor in horizontal direction. The fill factor of an image sensor array is the ratio of a pixel's light sensitive area to its total area. See https://en.wikipedia.org/wiki/Fill_factor_(image_sensor). The value must be between 0 and 1, where 1 depicts a 100 percent. The higher the fill factor, the more sensitive the sensor." ;
    ssno:standardName "sensor_pixel_width_fill_factor" ;
    ssno:unit <http://qudt.org/vocab/unit/UNITLESS> .

local:signal_to_noise_ratio a ssno:StandardName ;
    ssno:description "The signal to noise ratio of the PIV evaluation." ;
    ssno:standardName "signal_to_noise_ratio" ;
    ssno:unit <http://qudt.org/vocab/unit/UNITLESS> .

local:standard_deviation_of_image_particle_diameter a ssno:StandardName ;
    ssno:description "Standard deviation of the image particle diameters in the flow. The diameter of an image particle approximated by a circular Gaussian intensity bell, where the diameter is defined at the e-2 intensity, thus the particle diameter is equal to 4 sigma of the Gaussian distribution. Note, that there might be other definitions, too, however, this one is the most common one in literature." ;
    ssno:standardName "standard_deviation_of_image_particle_diameter" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:standard_deviation_of_seeding_particle_size a ssno:StandardName ;
    ssno:description "The standard deviation of the seeding particles sizes." ;
    ssno:standardName "standard_deviation_of_seeding_particle_size" ;
    ssno:unit <http://qudt.org/vocab/unit/M> .

local:third_peak_x_displacement a ssno:StandardName ;
    ssno:description "Displacement is a vector quantity. X indicates the component in x-axis direction. This component is an in-plane component. The displacement is computed based on the third peak of the cross-correlation function." ;
    ssno:standardName "third_peak_x_displacement" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:third_peak_y_displacement a ssno:StandardName ;
    ssno:description "Displacement is a vector quantity. Y indicates the component in y-axis direction. This component is an in-plane component. The displacement is computed based on the third peak of the cross-correlation function." ;
    ssno:standardName "third_peak_y_displacement" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:x_coordinate a ssno:StandardName ;
    ssno:description "The spatial coordinate in horizontal direction in the global coordinate system, which is aligned with the sensor orientation. The sensor width therefore corresponds to sensor size in direction of x_coordinate." ;
    ssno:standardName "x_coordinate" ;
    ssno:unit <http://qudt.org/vocab/unit/M> .

local:x_derivative_of_x_velocity a ssno:StandardName ;
    ssno:description "The standard name x_derivative_of_x_velocity denotes the derivative of the x component of the velocity vector with respect to the x coordinate." ;
    ssno:standardName "x_derivative_of_x_velocity" ;
    ssno:unit <http://qudt.org/vocab/unit/PER-SEC> .

local:x_derivative_of_y_velocity a ssno:StandardName ;
    ssno:description "The standard name x_derivative_of_y_velocity denotes the derivative of the y component of the velocity vector with respect to the x coordinate." ;
    ssno:standardName "x_derivative_of_y_velocity" ;
    ssno:unit <http://qudt.org/vocab/unit/PER-SEC> .

local:x_derivative_of_z_velocity a ssno:StandardName ;
    ssno:description "The standard name x_derivative_of_z_velocity denotes the derivative of the z component of the velocity vector with respect to the x coordinate." ;
    ssno:standardName "x_derivative_of_z_velocity" ;
    ssno:unit <http://qudt.org/vocab/unit/PER-SEC> .

local:x_displacement a ssno:StandardName ;
    ssno:description "Displacement is a vector quantity and describes the particle displacement in pixels in the image plane. x indicates a vector component along the grid x-axis, positive with increasing x." ;
    ssno:standardName "x_displacement" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:x_final_interrogation_window_overlap_size a ssno:StandardName ;
    ssno:description "Width of a final PIV interrogation window overlap." ;
    ssno:standardName "x_final_interrogation_window_overlap_size" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:x_final_interrogation_window_size a ssno:StandardName ;
    ssno:description "Width of a final PIV interrogation window." ;
    ssno:standardName "x_final_interrogation_window_size" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:x_image_coordinate a ssno:StandardName ;
    ssno:description "The spatial coordinate in horizontal direction. The origin is at the top left." ;
    ssno:standardName "x_image_coordinate" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:x_initial_interrogation_window_size a ssno:StandardName ;
    ssno:description "Width of a initial PIV interrogation window (in case of a multi-grid PIV algorithm)." ;
    ssno:standardName "x_initial_interrogation_window_size" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:x_inplane_velocity a ssno:StandardName ;
    ssno:description "The standard name inplane_velocity denotes the magnitude of a two-dimensional velocity vector. Plane is spanned by the x and y axis of PIV plane. The PIV plane is the plane illuminated by the laser. x indicates a vector component along the grid x-axis, positive with increasing x." ;
    ssno:standardName "x_inplane_velocity" ;
    ssno:unit <http://qudt.org/vocab/unit/M-PER-SEC> .

local:x_origin_offset a ssno:StandardName ;
    ssno:description "The physical offset of the x_image_origin_coordinate in the global coordinate system." ;
    ssno:standardName "x_origin_offset" ;
    ssno:unit <http://qudt.org/vocab/unit/M> .

local:x_pixel_coordinate a ssno:StandardName ;
    ssno:description "Pixel coordinate in horizontal direction. It depicts the position of each velocity vector in the result data in pixel unit, where the origin is at the lower left. The coordinate is aligned with the physical coordinate axis (x_coordinate), not the image coordinate (x_image_coordinate) which has its origin in the top left of the image." ;
    ssno:standardName "x_pixel_coordinate" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:x_velocity a ssno:StandardName ;
    ssno:description "Velocity is defined as a three-dimensional velocity vector. x indicates a vector component along the grid x-axis, positive with increasing x." ;
    ssno:standardName "x_velocity" ;
    ssno:unit <http://qudt.org/vocab/unit/M-PER-SEC> .

local:y_coordinate a ssno:StandardName ;
    ssno:description "The spatial coordinate in vertical direction in the global coordinate system, which is aligned with the sensor orientation. The sensor width therefore corresponds to sensor size in direction of y_coordinate." ;
    ssno:standardName "y_coordinate" ;
    ssno:unit <http://qudt.org/vocab/unit/M> .

local:y_derivative_of_x_velocity a ssno:StandardName ;
    ssno:description "The standard name y_derivative_of_x_velocity denotes the derivative of the x component of the velocity vector with respect to the y coordinate." ;
    ssno:standardName "y_derivative_of_x_velocity" ;
    ssno:unit <http://qudt.org/vocab/unit/PER-SEC> .

local:y_derivative_of_y_velocity a ssno:StandardName ;
    ssno:description "The standard name y_derivative_of_y_velocity denotes the derivative of the y component of the velocity vector with respect to the y coordinate." ;
    ssno:standardName "y_derivative_of_y_velocity" ;
    ssno:unit <http://qudt.org/vocab/unit/PER-SEC> .

local:y_derivative_of_z_velocity a ssno:StandardName ;
    ssno:description "The standard name y_derivative_of_z_velocity denotes the derivative of the z component of the velocity vector with respect to the y coordinate." ;
    ssno:standardName "y_derivative_of_z_velocity" ;
    ssno:unit <http://qudt.org/vocab/unit/PER-SEC> .

local:y_displacement a ssno:StandardName ;
    ssno:description "y indicates a vector component along the grid y-axis, positive with increasing y.Displacement is a vector quantity and describes the particle displacement in pixels in the image plane." ;
    ssno:standardName "y_displacement" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:y_final_interrogation_window_overlap_size a ssno:StandardName ;
    ssno:description "The height of a final PIV interrogation window overlap." ;
    ssno:standardName "y_final_interrogation_window_overlap_size" ;
    ssno:unit <http://qudt.org/vocab/unit/PIXEL> .

local:y_final_interrogation_window_size a ssno:StandardName ;
    ssno:description "The height of the final PIV interrogation window." ;
